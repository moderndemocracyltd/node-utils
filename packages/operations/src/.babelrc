{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "6.10"
        },
        "include": [
          "transform-es2015-classes",
          "transform-es2015-destructuring",
          "transform-regenerator"
        ],
        "useBuiltIns": true
      }
    ]
  ],
  "plugins": [
    [
      "transform-runtime",
      {
        "polyfill": false,
        "regenerator": true
      }
    ]
  ]
}